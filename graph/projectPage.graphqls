type ProjectPageHttp {
    projectPageId: String!
    lastModified: String!
    projectId: String!
    instruction: String!
    notes: String!
    preview: String!
    linkScratch: String!
    title: String!
    isShared: Boolean!
}

extend type Query {
    GetProjectPageById(projectPageID: String!): ProjectPageHttp!
    GetAllProjectPagesByUserID(userID: String!): [ProjectPageHttp!]!
    GetAllProjectPagesByAccessToken: [ProjectPageHttp!]!
}