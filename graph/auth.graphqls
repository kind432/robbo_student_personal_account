input SignInInput {
	email: String!
	password: String!
	userRole: Int!
}

input SignUpRequest {
	email: String!
	password: String!
	nickname: String!
	firstname: String!
	lastname: String!
	middlename: String!
}

type SignInResponse {
	accessToken: String!
}

union SignInResult = Error | SignInResponse

extend type Mutation {
	SignIn(input: SignInInput!): SignInResult!
	SignUpRequest(input: NewStudent!): Error!
	SignOut: Error
	Refresh: SignInResult
}